{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/preload/*.d.ts",
    "src/main/*.d.ts",
    "src/lib/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "useUnknownInCatchVariables": false,
    "strictPropertyInitialization": false,
    "paths": {
      "@renderer/*": ["src/renderer/src/*"]
    }
  }
}
