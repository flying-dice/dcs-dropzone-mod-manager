{
  "name": "dcs-dropzone",
  "version": "1.6.0",
  "description": "A community Mod Manager for DCS World",
  "main": "./out/main/index.js",
  "author": "Flying Dice",
  "private": true,
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm run build"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            "dist/*.exe",
            "dist/*.yml",
            "dist/*.yaml"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "rimraf dist && electron-vite build && electron-builder --win --config",
    "generate-client": "orval --config ./orval.config.js",
    "test": "vitest --no-watch"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@fontsource/dela-gothic-one": "^5.0.17",
    "@fontsource/genos": "^5.0.18",
    "@fontsource/montserrat": "^5.0.16",
    "@mantine/code-highlight": "^7.4.0",
    "@mantine/core": "^7.4.0",
    "@mantine/form": "^7.4.0",
    "@mantine/hooks": "^7.4.0",
    "@mantine/modals": "^7.4.0",
    "@mantine/notifications": "^7.4.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@types/bytes": "^3.1.4",
    "aigle": "^1.14.1",
    "axios": "^1.6.4",
    "better-sqlite3": "^9.6.0",
    "bytes": "^3.1.2",
    "chokidar": "^3.6.0",
    "electron-trpc": "^0.5.2",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "glob": "^11.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "marked": "^11.1.1",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "ms": "^2.1.3",
    "nodejs-file-downloader": "^4.12.1",
    "object-hash": "^3.0.0",
    "octokit": "^3.1.2",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.21.1",
    "react-use": "^17.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swr": "^2.2.4",
    "ts-pattern": "^5.0.6",
    "typeorm": "^0.3.20",
    "unzipper": "^0.10.14",
    "winston": "^3.13.0",
    "workerpool": "^9.1.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@swc/core": "^1.7.0",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/unzipper": "^0.10.9",
    "@vitejs/plugin-react": "^4.0.4",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "orval": "^6.23.0",
    "prettier": "^3.0.2",
    "semantic-release": "^22.0.12",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^1.4.0"
  }
}
